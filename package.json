    "file-loader": "3.0.1",
    "html-webpack-plugin": "3.2.0",
    "nodemon": "1.18.10",
    "style-loader": "0.23.0",
    "url-loader": "1.1.2",
    "webpack": "4.5.0",
    "webpack-cli": "3.0.8",
    "webpack-dev-server": "3.1.3"
  }
