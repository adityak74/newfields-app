{
  "name": "newfields-app",
  "version": "1.0.0",
  "description": "Newfields-Data Collection App",
  "main": "src/server/index.js",
  "scripts": {
    "build": "webpack --mode production",
    "clean:i": "npm run clean:mac && npm i",
    "clean:mac": "rm -rf node_modules *-lock.json",
    "client": "webpack-dev-server --mode development --devtool inline-source-map --hot",
    "dev": "concurrently \"npm run server\" \"npm run client\"",
    "start": "npm run build && babel-node src/server/index.js",
    "server": "nodemon --exec babel-node src/server/index.js",
    "server:dev": "NODE_ENV=development nodemon --exec babel-node src/server/index.js"
  },
  "author": "Aditya Karnam, Gaganpreet Singh",
  "license": "ISC",
  "dependencies": {
    "@material-ui/core": "3.9.2",
    "aws-sdk": "^2.422.0",
    "babel-polyfill": "6.26.0",
    "bcrypt-nodejs": "0.0.3",
    "body-parser": "^1.18.3",
    "connect-flash": "^0.1.1",
    "convict": "^4.4.1",
    "cookie-parser": "^1.4.4",
    "cors": "^2.8.5",
    "dotenv": "^6.2.0",
    "ejs": "^2.6.1",
    "express": "4.16.3",
    "express-session": "^1.15.6",
    "express-winston": "^3.1.0",
    "joi": "14.3.1",
    "joi-date-extensions": "^1.2.0",
    "joi-phone-number": "^2.1.1",
    "joi-postalcode": "^1.0.0",
    "mysql": "^2.16.0",
    "passport": "^0.4.0",
    "passport-local": "^1.0.0",
    "react": "16.5.2",
    "react-dom": "16.5.2",
    "recompose": "0.30.0",
    "redux": "4.0.1",
    "winston": "^3.2.1",
    "xss": "^1.0.3"
  },
  "devDependencies": {
    "@babel/cli": "^7.2.3",
    "@babel/core": "7.0.0",
    "@babel/node": "^7.2.2",
    "@babel/plugin-proposal-class-properties": "7.0.0",
    "@babel/preset-env": "7.0.0",
    "@babel/preset-react": "7.0.0",
    "babel-eslint": "10.0.0",
    "babel-loader": "8.0.0",
    "clean-webpack-plugin": "1.0.0",
    "concurrently": "4.0.0",
    "css-loader": "2.0.0",
