    "apollo-link-schema": "^1.2.2",
    "apollo-server-express": "^2.5.0",
    "async": "^2.6.2",
    "aws-sdk": "^2.422.0",
    "babel-polyfill": "6.26.0",
    "bcrypt-nodejs": "0.0.3",
    "body-parser": "^1.18.3",
    "connect-flash": "^0.1.1",
    "connect-redis": "^3.4.1",
    "convict": "^4.4.1",
    "cookie-parser": "^1.4.4",
    "cors": "^2.8.5",
    "dotenv": "^6.2.0",
    "ejs": "^2.6.1",
    "express": "4.16.3",
    "express-fileupload": "^1.1.4",
    "express-graphql": "^0.8.0",
    "express-session": "^1.15.6",
    "express-winston": "^3.1.0",
    "generate-password": "^1.4.1",
    "graphql": "^14.3.0",
    "graphql-tag": "^2.10.1",
    "graphql-tools": "^4.0.4",
    "joi": "14.3.1",
    "joi-date-extensions": "^1.2.0",
